{
  "name": "vusa",
  "version": "1.0.0",
  "description": "san runtime adapter for vue components.",
  "main": "index.js",
  "scripts": {
    "build:compiler": "BABEL_ENV=compiler webpack --config tool/webpack.compiler.config.js",
    "build:runtime": "BABEL_ENV=runtime webpack --config tool/webpack.runtime.config.js && BABEL_ENV=runtime webpack --config tool/webpack.runtime.min.config.js",
    "build": "npm run build:compiler && npm run build:runtime",
    "test": "jest",
    "predev": "npm run build",
    "dev": "webpack-dev-server --config tool/webpack.dev.config.js",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "keywords": [
    "san",
    "vue"
  ],
  "author": "cxtom (cxtom2008@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "atom-expression-compiler": "^2.2.3",
    "escape-quotes": "^1.0.2",
    "htmlparser2": "^4.1.0",
    "san": "^3.8.3",
    "to-single-quotes": "^3.0.0",
    "vue-template-compiler": "2.5.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.5",
    "css-loader": "^3.4.2",
    "domutils": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lerna": "^3.20.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "vue": "2.5.22",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
